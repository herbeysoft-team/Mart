{
  "expo": {
    "name": "trowmart",
    "slug": "trowmart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.herbeysoft.trowmart",
      "config":{
        "googleMapsApikey":"AIzaSyBOSZNsjlfibDcXrOnPCpwvrqET6oDqN2M"
      }
    },
    "android": {
      
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config":{
        "package": "com.herbeysoft.trowmart",
        "googleMaps": {
          "apiKey": "AIzaSyBOSZNsjlfibDcXrOnPCpwvrqET6oDqN2M"
        }
      },
      "permissions":[
        "android.permission.ACCESS_COASE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-image-picker", { "photosPermission": "Allow TrowMart to access your gallery" }],
      ["expo-camera", { "cameraPermission": "Allow TrowMart to access your camera." }],
      ["expo-location",{"locationAlwaysAndWhenInUsePermission": "Allow TrowMart to use your location."}]
    ]
  }
}
